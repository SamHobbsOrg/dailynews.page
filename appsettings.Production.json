{
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5030",
        "Certificate": {
          "Path": "/etc/ssl/dailynews.crt",
          "KeyPath": "/etc/ssl/dailynews.pem"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
